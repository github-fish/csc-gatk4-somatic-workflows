[2019-08-28 08:39:16,98] [info] BackgroundConfigAsyncJobExecutionActor [734d1462Mutect2.Funcotate:NA:1]: set -e
export GATK_LOCAL_JAR=/root/gatk.jar

# Extract our data sources:
echo "Extracting data sources zip file..."
mkdir datasources_dir
tar zxvf /cromwell-executions/Mutect2/734d1462-cd55-473f-b3b6-128f822b1bfa/call-Funcotate/inputs/-771983168/funcotator_dataSources.v1.6.20190124s.tar.gz -C datasources_dir --strip-components 1
DATA_SOURCES_FOLDER="$PWD/datasources_dir"

# Handle gnomAD:
if false ; then
    echo "Enabling gnomAD..."
    for potential_gnomad_gz in gnomAD_exome.tar.gz gnomAD_genome.tar.gz ; do
        if [[ -f ${DATA_SOURCES_FOLDER}/${potential_gnomad_gz} ]] ; then
            cd ${DATA_SOURCES_FOLDER}
            tar -zvxf ${potential_gnomad_gz}
            cd -
        else
            echo "ERROR: Cannot find gnomAD folder: ${potential_gnomad_gz}" 1>&2
            false
        fi
    done
fi

# Run Funcotator:
gatk --java-options "-Xmx2000m" Funcotator \
    --data-sources-path $DATA_SOURCES_FOLDER \
    --ref-version hg19 \
    --output-file-format MAF \
    -R /cromwell-executions/Mutect2/734d1462-cd55-473f-b3b6-128f822b1bfa/call-Funcotate/inputs/-771983168/Homo_sapiens_assembly19.fasta \
    -V /cromwell-executions/Mutect2/734d1462-cd55-473f-b3b6-128f822b1bfa/call-Funcotate/inputs/2006702577/HCC1143-filtered.vcf \
    -O HCC1143-filtered.annotated.maf \
      \
    --annotation-default normal_barcode:HCC1143_BL \
    --annotation-default tumor_barcode:HCC1143 \
    --annotation-default Center:Unknown \
    --annotation-default source:Unknown \
    --transcript-selection-mode ALL \
     \
     \
     \
     \
     --remove-filtered-variants  \
    
# Make sure we have a placeholder index for MAF files so this workflow doesn't fail:
if [[ "MAF" == "MAF" ]] ; then
   touch HCC1143-filtered.annotated.maf.idx
fi
