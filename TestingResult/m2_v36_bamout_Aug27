echo "" > normal_name.txt

gatk --java-options "-Xmx3000m" GetSampleName -R /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-37/inputs/-771983168/Homo_sapiens_assembly19.fasta -I /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-37/inputs/-771983168/HCC1143.bam -O tumor_name.txt -encode
