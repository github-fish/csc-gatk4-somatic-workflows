    normal_command_line="-I /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-26/inputs/-771983168/HCC1143_normal.bam -normal cat normal_name.txt"
fi

gatk --java-options "-Xmx3000m" Mutect2 \
    -R /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-26/inputs/-771983168/Homo_sapiens_assembly19.fasta \
    $tumor_command_line \
    $normal_command_line \
    --germline-resource /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-26/inputs/-771983168/af-only-gnomad.raw.sites.vcf \
