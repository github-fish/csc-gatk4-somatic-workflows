tumor_command_line="-I /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-26/inputs/-771983168/HCC1143.bam -tumor cat tumor_name.txt"

if [[ ! -z "/cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-26/inputs/-771983168/HCC1143_normal.bam" ]]; then
