[2019-08-27 22:22:02,99] [info] BackgroundConfigAsyncJobExecutionActor [96fe5b5eMutect2.M2:3:1]: set -e

export GATK_LOCAL_JAR=/root/gatk.jar

# We need to create these files regardless, even if they stay empty
touch bamout.bam
touch f1r2.tar.gz
echo "" > normal_name.txt

gatk --java-options "-Xmx3000m" GetSampleName -R /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/Homo_sapiens_assembly19.fasta -I /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/HCC1143.bam -O tumor_name.txt -encode
tumor_command_line="-I /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/HCC1143.bam -tumor cat tumor_name.txt"

if [[ ! -z "/cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/HCC1143_normal.bam" ]]; then
    gatk --java-options "-Xmx3000m" GetSampleName -R /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/Homo_sapiens_assembly19.fasta -I /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/HCC1143_normal.bam -O normal_name.txt -encode
    normal_command_line="-I /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/HCC1143_normal.bam -normal cat normal_name.txt"
fi

gatk --java-options "-Xmx3000m" Mutect2 \
    -R /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/Homo_sapiens_assembly19.fasta \
    $tumor_command_line \
    $normal_command_line \
    --germline-resource /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/af-only-gnomad.raw.sites.vcf \
    -pon /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/Mutect2-exome-panel.vcf \
    -L /cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-730433008/0003-scattered.interval_list \
     \
    -O "output.vcf" \
    --bam-output bamout.bam \
    --f1r2-tar-gz f1r2.tar.gz \
    

### GetPileupSummaries
# These must be created, even if they remain empty, as cromwell doesn't support optional output
touch tumor-pileups.table
touch normal-pileups.table

if [[ ! -z "/cromwell-executions/Mutect2/96fe5b5e-005a-49b2-a686-847110d77d6c/call-M2/shard-3/inputs/-771983168/small_exac_common_3.vcf" ]]; then
