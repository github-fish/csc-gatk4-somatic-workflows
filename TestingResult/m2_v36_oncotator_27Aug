# local db dir is a directory and has been specified
if [[ -d "" ]]; then
    echo "Using local db-dir: "
    echo "THIS ONLY WORKS WITHOUT DOCKER!"
    ln -s  onco_dbdir
elif [[ "/cromwell-executions/Mutect2/bc490500-d47c-4358-a86d-7cf14686144b/call-oncotate_m2/inputs/-771983168/oncotator_v1_ds_April052016.tar.gz" == *.tar.gz ]]; then
    echo "Using given tar file: /cromwell-executions/Mutect2/bc490500-d47c-4358-a86d-7cf14686144b/call-oncotate_m2/inputs/-771983168/oncotator_v1_ds_April052016.tar.gz"
