tumor_command_line="-I /cromwell-executions/Mutect2/bc490500-d47c-4358-a86d-7cf14686144b/call-M2/shard-3/inputs/-771983168/HCC1143.bam -tumor cat tumor_name.txt"

if [[ ! -z "/cromwell-executions/Mutect2/bc490500-d47c-4358-a86d-7cf14686144b/call-M2/shard-3/inputs/-771983168/HCC1143_normal.bam" ]]; then
