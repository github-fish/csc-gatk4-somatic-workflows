    tar zxvf /cromwell-executions/Mutect2/bc490500-d47c-4358-a86d-7cf14686144b/call-oncotate_m2/inputs/-771983168/oncotator_v1_ds_April052016.tar.gz -C onco_dbdir --strip-components 1
else
