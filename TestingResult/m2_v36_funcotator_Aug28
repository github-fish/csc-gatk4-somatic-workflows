set -e

# local db dir is a directory and has been specified
if [[ -d "" ]]; then
    echo "Using local db-dir: "
    echo "THIS ONLY WORKS WITHOUT DOCKER!"
    ln -s  onco_dbdir
elif [[ "/cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-oncotate_m2/inputs/-771983168/oncotator_v1_ds_April052016.tar.gz" == *.tar.gz ]]; then
    echo "Using given tar file: /cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-oncotate_m2/inputs/-771983168/oncotator_v1_ds_April052016.tar.gz"
    mkdir onco_dbdir
    tar zxvf /cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-oncotate_m2/inputs/-771983168/oncotator_v1_ds_April052016.tar.gz -C onco_dbdir --strip-components 1
else
    echo "Downloading and installing oncotator datasources from Broad FTP site..."
    # Download and untar the db-dir
    wget ftp://gsapubftp-anonymous@ftp.broadinstitute.org/bundle/oncotator/oncotator_v1_ds_April052016.tar.gz
    tar zxvf oncotator_v1_ds_April052016.tar.gz
    ln -s oncotator_v1_ds_April052016 onco_dbdir
fi

/root/oncotator_venv/bin/oncotator --db-dir onco_dbdir/ -c $HOME/tx_exact_uniprot_matches.AKT1_CRLF2_FGFR1.txt  \
    -v /cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-oncotate_m2/inputs/-1654017850/HCC1143-filtered.vcf HCC1143.maf.annotated hg19 -i VCF -o TCGAMAF --skip-no-alt --collapse-number-annotations --log_name oncotator.log \
    -a Center:Unknown \
    -a source:Unknown \
    -a normal_barcode:HCC1143_BL \
    -a tumor_barcode:HCC1143 \
    --default_config /cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-oncotate_m2/inputs/-771983168/onco_config.txt \
