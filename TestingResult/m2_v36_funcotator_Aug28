if [[ ! -z "/cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-M2/shard-17/inputs/-771983168/HCC1143_normal.bam" ]]; then
    gatk --java-options "-Xmx3000m" GetSampleName -R /cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-M2/shard-17/inputs/-771983168/Homo_sapiens_assembly19.fasta -I /cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-M2/shard-17/inputs/-771983168/HCC1143_normal.bam -O normal_name.txt -encode
    normal_command_line="-I /cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-M2/shard-17/inputs/-771983168/HCC1143_normal.bam -normal cat normal_name.txt"
fi

gatk --java-options "-Xmx3000m" Mutect2 \
    -R /cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-M2/shard-17/inputs/-771983168/Homo_sapiens_assembly19.fasta \
    $tumor_command_line \
    $normal_command_line \
    --germline-resource /cromwell-executions/Mutect2/52809c50-3cda-47e7-9734-07ed1e768f61/call-M2/shard-17/inputs/-771983168/af-only-gnomad.raw.sites.vcf \
